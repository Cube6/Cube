{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Audience": "http://localhost:30001;",
    "Issuer": "http://localhost:30000",
    "SecretKey": "http://www.cubesystem.com/secretkey"
  },
  "ConsulSetting": {
    "IsHttps": true,
    "ServiceName": "User.API",
    "ServiceIP": "localhost",
    "ServicePort": 30001,
    "ServiceHealthCheck": "/healthcheck",
    "ConsulAddress": "http://localhost:8500" //注意，docker容器内部无法使用localhost访问宿主机器，如果是控制台启动的话就用localhost
  }
}
